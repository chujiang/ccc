<?xml version="1.0" encoding="GBK"?>

<dictConfig name="crs">
	
	<static name="costType" >
		<item key="crs.costType_personal" value="P"/>
		<item key="crs.costType_company" value="C"/>
	</static>
	<static name="courseType" >
		<item key="common.filter_all" value=""/>
		<item key="crs.courseType_scorm" value="S"/>
		<item key="crs.courseType_noscorm" value="F"/>
	</static>
	
	<static name="control">
		<item key = "crs.courseInfo_control_system" value="0" />
		<item key="crs.courseInfo_control_course" value="1" />
	</static>
	
	<static name="courseInfoFilterItems">
		<item key="common.filter_all" value=""/>
