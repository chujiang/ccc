

<div>
<#macro repeat count>
	<#list 0..count as x>
	<#if (x < parameters.grade) >
		<img name="gread_img" src="${parameters.srcHeader}/images/wj-hs.png" style="width: 12px; height: 11px;" />
	<#else>
		<img name="gread_img" src="${parameters.srcHeader}/images/wj-huise.png" style="width: 12px; height: 11px;" />
	</#if>
	
	</#list>
</#macro>
<@repeat count=4 ; >
</@repeat>
</div>