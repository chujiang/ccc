package com.kingnod.etraining.common.webservices.bean;

import java.util.List;

public class WSQuestion {
	
	private Long folderId;
	private String type;
	private Short degree;
	private Float score;
	private String content;
	private List<WSQuestionAnswer> wsQuestionAnswers;
	
	public Long getFolderId() {
		return folderId;
	}
	public String getType() {
		return type;
	}
	public Short getDegree() {
		return degree;
	}
	public Float getScore() {
		return score;
	}
	public String getContent() {
		return content;
	}
	public void setFolderId(Long folderId) {
		this.folderId = folderId;
	}
	public void setType(String type) {
		this.type = type;
	}
	public void setDegree(Short degree) {
		this.degree = degree;
	}
	public void setScore(Float score) {
		this.score = score;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public List<WSQuestionAnswer> getWSQuestionAnswers() {
		return wsQuestionAnswers;
	}
	public void setWSQuestionAnswers(List<WSQuestionAnswer> wsQuestionAnswers) {
		this.wsQuestionAnswers = wsQuestionAnswers;
	}
	

}
