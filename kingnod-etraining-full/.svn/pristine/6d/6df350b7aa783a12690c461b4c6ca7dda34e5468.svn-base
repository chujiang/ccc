package com.kingnod.etraining.common.ui.components;

import java.io.Serializable;

@SuppressWarnings("serial")
public class ContextMenu implements Serializable {
	private String name;
	private String label;
	private String icon;
	private String src;
	private String openMode;
	private String target;

	public ContextMenu() {
	}

	public ContextMenu(String name, String label) {
		super();
		this.name = name;
		this.label = label;
	}

	@org.apache.struts2.json.annotations.JSON(name = "name")
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@org.apache.struts2.json.annotations.JSON(name = "label")
	public String getLabel() {
		return label;
	}

	public void setLabel(String label) {
		this.label = label;
	}

	@org.apache.struts2.json.annotations.JSON(name = "icon")
	public String getIcon() {
		return icon;
	}

	public void setIcon(String icon) {
		this.icon = icon;
	}

	@org.apache.struts2.json.annotations.JSON(name = "src")
	public String getSrc() {
		return src;
	}

	@org.apache.struts2.json.annotations.JSON(name = "openMode")
	public String getOpenMode() {
		return openMode;
	}

	@org.apache.struts2.json.annotations.JSON(name = "target")
	public String getTarget() {
		return target;
	}

	public void setSrc(String src) {
		this.src = src;
	}

	public void setOpenMode(String openMode) {
		this.openMode = openMode;
	}

	public void setTarget(String target) {
		this.target = target;
	}
}
