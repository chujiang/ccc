package com.kingnod.etraining.common.ui.components;

/**
 * @author liuxiaobin 存储Layout表格中的数据
 */
public class LayoutResult {
	/**
	 * 当前行是否结束
	 */
	private boolean isEnd;

	/**
	 * 当前行剩余列数
	 */
	private int leavinCols;

	public LayoutResult() {
	}

	public LayoutResult(boolean isEnd, int leavinCols) {
		super();
		this.isEnd = isEnd;
		this.leavinCols = leavinCols;
	}

	public boolean isEnd() {
		return isEnd;
	}

	public void setEnd(boolean isEnd) {
		this.isEnd = isEnd;
	}

	public int getLeavinCols() {
		return leavinCols;
	}

	public void setLeavinCols(int leavinCols) {
		this.leavinCols = leavinCols;
	}
}
