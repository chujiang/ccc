package com.kingnod.etraining.common.ui.tags;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import org.apache.struts2.components.Component;

import com.kingnod.etraining.common.ui.components.Button;
import com.opensymphony.xwork2.util.ValueStack;

public class ButtonTag extends LinkTag {

  private static final long serialVersionUID = -6503701873654126103L;
  
  public Component getBean(ValueStack stack, HttpServletRequest req, HttpServletResponse res) {
    return new Button(stack, req, res);
  }

}
