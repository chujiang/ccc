package com.kingnod.etraining.course.exception;

/**
 * Description
 *
 * @author Phillip Beauvoir
 * @version $Id: DocumentHandlerException.java,v 1.3 2007/07/15 20:28:49 phillipus Exp $
 */
public class DocumentHandlerException extends Exception {
	/**
	 * 
	 */
	public DocumentHandlerException() {
		super();
	}
	/**
	 * @param message
	 */
	public DocumentHandlerException(String message) {
		super(message);
	}
	/**
	 * @param message
	 * @param cause
	 */
	public DocumentHandlerException(String message, Throwable cause) {
		super(message, cause);
	}
	/**
	 * @param cause
	 */
	public DocumentHandlerException(Throwable cause) {
		super(cause);
	}
}
