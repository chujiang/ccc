
package com.kingnod.etraining.course.parsexmlutil;






/**
 * The XMLDocumentListener to notify when a JDOM Document has been changed
 *
 * @author Phillip Beauvoir
 * @version $Id: XMLDocumentListener.java,v 1.3 2007/07/15 20:27:46 phillipus Exp $
 */
public interface XMLDocumentListener {
    void elementAdded(XMLDocumentListenerEvent event);
    void elementRemoved(XMLDocumentListenerEvent event);
    void elementChanged(XMLDocumentListenerEvent event);
    void documentSaved(XMLDocument doc);
}
