CREATE TABLE ATY_ACTIVITY
(
	ACTIVITY_ID          NUMBER(18) NULL ,
	TYPE                 CHAR(4) DEFAULT  ''  NULL ,
	FOLDER_ID            NUMBER(18) NULL ,
	NAME                 VARCHAR2(100) NULL ,
	OBJECT_DETAILS       CLOB NULL ,
	RECORD_STATUS        CHAR(1) DEFAULT  ''  NULL ,
	UPDATE_COUNT         NUMBER(18) DEFAULT  0  NULL ,
	CREATOR_ID           NUMBER(18) NULL ,
	CREATE_DATE          DATE NULL ,
	UPDATER_ID           NUMBER(18) NULL ,
	UPDATE_DATE          DATE NULL ,
	GRADE                NUMBER(1) NULL ,
	RELEASE_DATE         DATE NULL ,
	COMPLETE_NUMBER      NUMBER(18) NULL ,
	ONLINE_NUMBER        NUMBER(18) NULL ,
	RELEASE_STAUTS       CHAR(1) NULL 
);

COMMENT ON COLUMN ATY_ACTIVITY.GRADE IS '活动星级';

COMMENT ON COLUMN ATY_ACTIVITY.RELEASE_DATE IS '发布日期';

COMMENT ON COLUMN ATY_ACTIVITY.COMPLETE_NUMBER IS '完成人数';

COMMENT ON COLUMN ATY_ACTIVITY.ONLINE_NUMBER IS '在线人数';

COMMENT ON COLUMN ATY_ACTIVITY.RELEASE_STAUTS IS '状态, P：编辑，C：审核中 R：发布';

CREATE UNIQUE INDEX ATY_ACTIVITY_PK ON ATY_ACTIVITY
(ACTIVITY_ID   ASC,TYPE   ASC);

CREATE TABLE ATY_ELEARNING
(
	ID                   NUMBER(18) NOT NULL ,
	FOLDER_ID            NUMBER(18) NULL ,
	COURSE_ID            NUMBER(18) NULL ,
	NAME                 VARCHAR2(100) NULL ,
	TRAINER              NUMBER(18) NULL ,
	LESSONS              NUMBER(9) NULL ,
	CREDIT               NUMBER(9) NULL ,
	FINISH_TIME          DATE NULL ,
	START_ENROLL_TIME    DATE NULL ,
	END_ENROLL_TIME      DATE NULL ,
	MIN_ENROLL_NUMBER    NUMBER(9) NULL ,
	MAX_ENROLL_NUMBER    NUMBER(9) NULL ,
	ENABLED_WAITING      CHAR(1) DEFAULT  ''  NULL ,
	ALLOW_LOGOUT         CHAR(1) DEFAULT  'Y'  NULL ,
	COURSE_TYPE_ID       NUMBER(18) NULL ,
	RELEASE_STAUTS       CHAR(1) DEFAULT  ''  NULL ,
	RECORD_STATUS        CHAR(1) DEFAULT  ''  NULL ,
	UPDATE_COUNT         NUMBER(18) DEFAULT  0  NULL ,
	CREATOR_ID           NUMBER(18) NULL ,
	CREATE_DATE          DATE NULL ,
	UPDATER_ID           NUMBER(18) NULL ,
	UPDATE_DATE          DATE NULL ,
	RELEASE_DATE         DATE NULL 
);

COMMENT ON COLUMN ATY_ELEARNING.FOLDER_ID IS '活动文件夹ID';

COMMENT ON COLUMN ATY_ELEARNING.COURSE_ID IS '课程ID';

COMMENT ON COLUMN ATY_ELEARNING.NAME IS '活动名称';

COMMENT ON COLUMN ATY_ELEARNING.TRAINER IS '讲师ID';

COMMENT ON COLUMN ATY_ELEARNING.LESSONS IS '课时';

COMMENT ON COLUMN ATY_ELEARNING.CREDIT IS '学分';

COMMENT ON COLUMN ATY_ELEARNING.FINISH_TIME IS '目标完成时间';

COMMENT ON COLUMN ATY_ELEARNING.START_ENROLL_TIME IS '报名开始时间';

COMMENT ON COLUMN ATY_ELEARNING.END_ENROLL_TIME IS '报名结束时间';

COMMENT ON COLUMN ATY_ELEARNING.MIN_ENROLL_NUMBER IS '最少报名人数';

COMMENT ON COLUMN ATY_ELEARNING.MAX_ENROLL_NUMBER IS '最多报名人数';

COMMENT ON COLUMN ATY_ELEARNING.ENABLED_WAITING IS '启用等待列表';

COMMENT ON COLUMN ATY_ELEARNING.ALLOW_LOGOUT IS '允许注销,Y:是,N:否';

COMMENT ON COLUMN ATY_ELEARNING.COURSE_TYPE_ID IS '课程类别ID';

COMMENT ON COLUMN ATY_ELEARNING.RELEASE_STAUTS IS '状态, P：编辑，C：审核中 R：发布';

COMMENT ON COLUMN ATY_ELEARNING.RELEASE_DATE IS '发布日期';

CREATE UNIQUE INDEX PK ON ATY_ELEARNING
(ID   ASC);

ALTER TABLE ATY_ELEARNING
	ADD CONSTRAINT  PK PRIMARY KEY (ID);

