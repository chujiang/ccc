CREATE  OR REPLACE  FUNCTION ALL_USERGROP (c_id number) RETURN VARCHAR2 IS
    v_path varchar2(2000);    --Return Full Path
    v_id ORG_USER_GROUP.id%type;
    v_name    ORG_USER_GROUP.name%type;
    v_count number(10);
    CURSOR c_path IS select a.id,a.name
 from ORG_USER_GROUP a,org_user_group_users b where b.user_group_id=a.id and b.user_id=c_id;
begin
    v_path :='';
    v_count :=0;
    select count(a.id) into v_count
       from ORG_USER_GROUP a,org_user_group_users b where b.user_group_id=a.id and b.user_id=c_id;
    IF v_count=0 THEN
        return('-');
    END IF;
    open c_path;
    loop
  fetch c_path into v_id,v_name;
  exit when c_path%notfound;
        v_path :=v_name||','||v_path;
    end loop;
    close c_path;
    v_path :=rtrim(v_path,',');
    return v_path;
end;
/



CREATE  OR REPLACE  FUNCTION COURSE_CATEGORY_NAME (c_id number) RETURN VARCHAR2 IS
    ----c_id is child id in tree.
    v_path varchar2(2000);    --Return Full Path
    v_id res_folder.id%type;
    v_name    res_folder.name%type;
    v_count number(10);
    CURSOR c_path IS  select t_folder.id,t_folder.name
 from cmn_object_relation t_c,res_folder t_folder
 where t_c.object2_id=t_folder.id and t_c.record_status='A' and t_c.object1_id=c_id;
begin
    v_path :='';
    v_count :=0;
    select count(t_folder.name) into v_count
      from cmn_object_relation t_c,res_folder t_folder
 where t_c.object2_id=t_folder.id and t_c.object1_id=c_id;
    IF v_count=0 THEN
        return('-');
    END IF;
    open c_path;
    loop
  fetch c_path into v_id,v_name;
  exit when c_path%notfound;
        v_path :=v_name||','||v_path;
    end loop;
    close c_path;
    v_path :=rtrim(v_path,',');
    return v_path;
end;
/



CREATE  OR REPLACE  FUNCTION GETDATAINTERSECTION (Str1 in varchar2, Str2 in varchar2)
  return varchar2 is
  result varchar2(4000);
begin
  select wm_concat(rn)
    into result
    from (select regexp_substr(replace(Str1,' ',''), '[^,]+', 1, rownum) rn
            from dual
          connect by rownum <= length(regexp_replace(replace(Str1,' ',''), '[^,]+')) + 1
          intersect
          select regexp_substr(replace(Str2,' ',''), '[^,]+', 1, rownum) rn
            from dual
          connect by rownum <= length(regexp_replace(replace(Str2,' ',''), '[^,]+')) + 1);
  return result;
end getDataIntersection;
/



